.box{
  width: 690rpx;
  height: 346rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}
.box-top{
  width: 100%;
  height: 100rpx;
  background: #FFBD78;
  border-radius: 10rpx 10rpx 0 0;
  float: left;
}
.box-top-left{
  width: 135rpx;
  height: 32rpx;
  float: left;
  margin: 34rpx 0 0 40rpx;
  font-size: 34rpx;
  color: #333333;
}
.box-top-right{
  width: 32rpx;
  height: 32rpx;
  float: right;
  margin: 35rpx 34rpx 0 0;
}
.box-top-right>image{
  width: 100%;
  height: 100%;
}
.box-bottom{
  width: 615rpx;
  height: 167rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.box-bottom-top{
  width: 615rpx;
  height: 27rpx;
  font-size: 28rpx;
  color: #333333;
}
.nature{
  font-size: 24rpx;
  color: #666666;
}