.box{
  width: 100%;
  height: 100rpx;
  background: white;
  display: flex;
  justify-content: center;
}
.box-left{
  width: 300rpx;
  height: 80rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  background: #DCDCDC;
  padding-left: 80rpx;
}
.box-center{
  width: 130rpx;
  height: 80rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  background: #DCDCDC;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
}