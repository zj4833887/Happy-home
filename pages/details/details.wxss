.search-box{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}
.search{
  width: 30%;
  height:50rpx;
  border-radius: 20rpx;
  margin-right: 20rpx;
  text-align: center;
  background: #ffffff;
}
.list-box{
  margin-left: 10rpx;
  font-size: 30rpx;
}
.listBox{
  width: 90%;
  height: 200rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid #000000;
  display: flex;
  justify-content: start;
  background: #ffffff;
  margin-top: 10rpx;
}
.listBox-left{
  width: 150rpx;
  height: 100%;
}
.listBox-left-img{
  width: 130rpx;
  height: 130rpx;
  background: #18927e;
  margin-top: 31rpx;
  margin-left: 20rpx;
}
.listBox-left-img>image{
  width: 100%;
  height: 100%;
}
.listBox-center-wz{
  font-size: 30rpx;
  margin-top: 20rpx;
}
.listBox-center-zx{
  width: 150rpx;
  height: 50rpx;
  background: #ffffff;
  margin-top: 50rpx;
  margin-left: 10rpx;
  float: left;
  font-size: 28rpx;
}
.listBox-center{
  width: 100%;
  height: 100%;
  margin-left: 10rpx;
}
.btn{
  width: 100rpx;
  height: 100rpx;
  background: skyblue;
  margin-right: 20rpx;
  margin-top: 50rpx;
  font-size: 20rpx;
  text-align: center;
  line-height: 100rpx;
}
